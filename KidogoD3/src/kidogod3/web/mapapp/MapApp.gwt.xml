<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="MapApp">
	<inherits name="com.google.gwt.user.User" />
	
	<inherits name="com.google.gwt.activity.Activity" />

	<inherits name='com.google.gwt.maps.Maps' />

	<inherits name="kidogod3.core.items.Items" />
	<inherits name="kidogod3.core.users.Users" />
	<inherits name="kidogod3.core.clientchannel.ClientChannel" />
	
	<source path="client" />
	<entry-point class="kidogod3.web.mapapp.client.MapApp" />
	
	<script src="http://maps.google.com/maps/api/js?sensor=false" />
</module>